{
  "compilerOptions": {
    "target": "es5",     
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext","es2015", "es2016"],
    "moduleResolution": "node",
    "module": "commonjs",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true
  }
}
